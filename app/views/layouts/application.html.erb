  <!DOCTYPE html>
  <html>

  <head>
    <title>TestApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if logged_in %>
    <%= link_to 'Log out', '/signout', method: :delete %>
    <% end %>
    <p id="notice"><%= notice %></p>
    <%= yield %>
  </body>

  </html>